directories:
  watch: "/tmp/test_dirs/watch"
  quarantine: "/tmp/test_dirs/quarantine"
  organized: "/tmp/test_dirs/organized"
  tmp_unzip: "/tmp/test_dirs/tmp_unzip"
  logs: "/tmp/test_dirs/orchestrator.log"

virus_scanning:
  clamscan_path: "clamscan"
  virustotal_api_key: ""   # Put your API key here, or leave blank to skip VT

archive_extensions:
  - ".zip"
  - ".tar"
  - ".tar.gz"
  - ".tgz"
  - ".tar.bz2"
  - ".tbz2"
  - ".rar"
  - ".7z"
  - ".gz"
  - ".bz2"
  - ".xz"
  - ".iso"
  - ".cab"
  - ".arj"
  - ".lzh"
  - ".Z"
  - ".ace"

log_level: "INFO"
metadata_extensions:
  - ".pdf"
  - ".docx"
  - ".jpg"
  - ".jpeg"
  - ".png"
  - ".txt"
  - ".tiff"
  - ".bmp"
  - ".dcm"
